        </div>
        <div class="go-next bottom-arrow">
          <h3>{{nav_bottom_arrow.next}}</h3>
          <div class="arrow"></div>
        </div>
    </div>

    <div class="slide" id="slide-8">
        <div class="centered">
            <!-- <h4 class="step">Step 5 of 5</h4> -->
            <h1>{{slide08.h1}}</h1>
            <!-- <img class="mascot monster" src="assets/png/no1.png"> -->
            <div class="action-choice">
              <h4>{{slide08.option01}}</h4>
              <p class="no-local-tr">
                 <a href="info/resources.html">{{slide08.no_local_tr.a}}</a>
                 {{slide08.no_local_tr.p}}
              </p>

              <p class="yes-local-tr hidden">
                {{slide08.yes_local_tr.p}}
                <a href="#open-modal"> {{slide08.yes_local_tr.a}} </a>
              </p>

              <h4>{{slide08.option02}}</h4>
              <p>{{slide08.p1}}
                <a href="http://www.nyshcr.org/Forms/Rent/ra89.pdf" target="_blank">{{slide08.a}}</a>
                {{slide08.p2}}
              </p>
            </div>
        </div>
        <div class="go-next bottom-arrow">
          <h3>{{nav_bottom_arrow.next}}</h3>
          <div class="arrow"></div>
        </div>
    </div>

    <div class="slide" id="slide-9">
        <div class="centered v-centered">
            <h1 class="end">{{slide09.h1}}</h1>
