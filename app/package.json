{
  "name": "am-i-rent-stabilized",
  "version": "1.0.0",
  "description": "Web app to inform nyc residents about rent stabilization",
  "private": true,
  "author": {
    "name": "Chris Henrick",
    "email": "chrishenrick@gmail.com",
    "url": "http://clhenrick.io"
  },
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:preloaded-state": "npm start -- --env.USE_PRELOADED_STATE=true",
    "start:prod": "serve dist",
    "webpack:debug": "webpack --debug=true --display-error-details=true --verbose=true --mode=development --env.NODE_ENV=development --env.USE_REDUX_LOGGER=true",
    "prod": "webpack --mode=production --env.NODE_ENV=production",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/clhenrick/am-i-rent-stabilized"
  },
  "keywords": [
    "rent-stabilization",
    "nyc",
    "civic-tech"
  ],
  "browserslist": {
    "production": [
      "> 0.25%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,vue,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest",
      "\\.hbs$": "jest-handlebars"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gsap)/)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.0",
    "@eslint/js": "^9.22.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.22.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-webpack-plugin": "^5.0.0",
    "gh-pages": "^2.2.0",
    "globals": "^16.0.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-handlebars": "^1.0.1",
    "lint-staged": "^10.1.5",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "stylelint-webpack-plugin": "^5.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "5",
    "webpack-cli": "5",
    "webpack-dev-server": "4"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "cross-fetch": "^3.1.5",
    "d3-geo": "^2.0.1",
    "d3-tile": "^1.0.0",
    "gsap": "^3.12.2",
    "handlebars": "4.7.7",
    "innersvg-polyfill": "^0.0.5",
    "lodash.throttle": "^4.1.1",
    "normalize.css": "^8.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.58.0",
    "single-line-string": "^0.0.2"
  },
  "packageManager": "yarn@4.7.0"
}
